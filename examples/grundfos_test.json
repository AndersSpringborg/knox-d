                            "__class__": "items",
                            "properties": {
                                "page": "4",
                                "header": "Preinstallation",

                                "paragraphs": {
                                    "type": "array",
                                    "items": [
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "4",
                                                "text": "If the pump is to be installed in a new well, the well should be fully developed and bailed or blown free of cuttings and sand."
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "5",
                                                "text": "Submersible well pumps are designed for pumping clear, cold water"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Warning Before carrying out maintenance and service on lifting stations used for pumping liquids which might be hazardous to health, make sure that the lifting station has been thoroughly flushed with clean water and that the outlet pipe has been drained"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Rinse the parts in water after dismantling"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Make sure that the isolating valves have been closed"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "The work must be carried out in accordance with local regulations"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Installation and operation must comply with local regulations and accepted codes of good practice"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Warning The lifting eye is only intended for lifting the pump"
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "6",
                                                "text": "Never lift or lower the lifting station by means of the lifting eye"
                                            }
                                        }
                                    ]
                                }
                            }
                        },
                        {
                            "__class__": "items",
                            "properties": {
                                "page": "8",
                                "header": "Installing the pump",

                                "paragraphs": {
                                    "type": "array",
                                    "items": [
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "9",
                                                "text": "To install the pump, attach the enclosed data plate sticker at the well head."
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "page": "5",
                                                "text": "During start-up and operation, the pump must always be completely submerged in water."
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            }
